int	ft_putnbr_fd(int n, int fd)
{
	int i;
	i = 0;
	if (n >= 16)
	{
		i = ft_putnbr_fd((n / 16), fd);
		ft_putnbr_fd((n % 16), fd);
	}
	if (n <= 9)
	{
		ft_putchar_fd ('0' + n, fd);
	}
		
	if (n >= 10 && n < 16)
	{
		ft_putchar_fd('a' - 10 + n, fd);
	}
	i++;
	return(i);
}

#include <stdio.h>
int	main()
{
	int i = 17;
	printf("\nvalue %d", ft_putnbr_fd(i, 1));	
	return 0;
}